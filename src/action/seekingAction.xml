<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />

    <package name="seeking" namespace="/seeking" extends="struts-default, json-default">
    
		<action name="toSeekingMain">
			<result>seekingMain.jsp</result>
		</action>
		<action name="getRecruit_list" class="squarize.action.seekingAction" method="getRecruit_list">
			<result type="chain">toSeekingMain</result>
		</action>
		
		
		
    </package>

</struts>
